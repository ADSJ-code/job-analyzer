<div id="<%= dom_id job %>">
  <p>
    <strong>Title:</strong>
    <%= job.title %>
  </p>

  <p>
    <strong>Company name:</strong>
    <%= job.company_name %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= job.location %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= job.description %>
  </p>

  <p>
    <strong>Job url:</strong>
    <%= job.job_url %>
  </p>

  <p>
    <strong>Company logo url:</strong>
    <%= job.company_logo_url %>
  </p>

  <p>
    <strong>Company website:</strong>
    <%= job.company_website %>
  </p>

</div>
